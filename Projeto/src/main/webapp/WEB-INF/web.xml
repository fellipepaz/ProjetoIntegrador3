        <servlet-name>ExcluirProdutoServlet</servlet-name>
        <url-pattern>/ExcluirProdutoServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PesquisarProdutoServlet</servlet-name>
        <servlet-class>br.senac.servlet.PesquisarProdutoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisarProdutoServlet</servlet-name>
        <url-pattern>/PesquisarProdutoServlet</url-pattern>
    </servlet-mapping>
