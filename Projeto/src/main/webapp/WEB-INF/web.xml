        <servlet-name>AtualizarProdutoServlet</servlet-name>
        <url-pattern>/AtualizarProdutoServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CadastrarProduto</servlet-name>
        <servlet-class>br.senac.servlet.CadastrarProduto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarProduto</servlet-name>
        <url-pattern>/produtos/CadastrarProduto</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ExcluirClienteServlet</servlet-name>
        <servlet-class>br.senac.servlet.ExcluirClienteServlet</servlet-class>
