        <servlet-name>ExcluirEstoqueServlet</servlet-name>
        <servlet-class>br.senac.servlet.ExcluirEstoqueServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarClienteServlet</servlet-name>
        <url-pattern>/CadastrarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AtualizarClienteServlet</servlet-name>
        <servlet-class>br.senac.servlet.AtualizarClienteServlet</servlet-class>
