        <servlet-name>VenderProdutoServlet</servlet-name>
        <servlet-class>br.senac.servlet.VenderProdutoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VenderProdutoServlet</servlet-name>
        <url-pattern>/VenderProdutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarProdutos</servlet-name>
        <url-pattern>/ListarProdutos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarEstoque</servlet-name>
        <url-pattern>/estoque/CadastrarEstoque</url-pattern>
